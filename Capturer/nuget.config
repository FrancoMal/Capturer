<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Configuración de NuGet para deshabilitar telemetrías -->
  <config>
    <!-- Deshabilitar telemetrías de NuGet -->
    <add key="disableSourceControlIntegration" value="true" />
    <add key="dependencyVersion" value="Highest" />
    <add key="automaticPackageRestore" value="true" />
    <add key="skipInvalidConfigurations" value="true" />
    
    <!-- Configuración de privacidad -->
    <add key="signatureValidationMode" value="accept" />
    <add key="trustLevel" value="0" />
  </config>
  
  <packageSources>
    <!-- Fuentes de paquetes sin telemetrías adicionales -->
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  
  <packageRestore>
    <!-- Restauración sin reportes de uso -->
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>
  
  <bindingRedirects>
    <add key="skip" value="False" />
  </bindingRedirects>
  
  <packageManagement>
    <add key="format" value="0" />
    <add key="disabled" value="False" />
  </packageManagement>
  
  <!-- Configuración adicional de seguridad y privacidad -->
  <trustedSigners>
    <!-- Sin confianza automática en certificados para evitar reportes -->
  </trustedSigners>
</configuration>